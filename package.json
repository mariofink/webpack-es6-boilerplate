        "es2015"
    ],
    "author": "Mario Fink",
    "license": "MIT",
    "devDependencies": {
        "babel-core": "^6.23.1",
        "babel-loader": "^6.3.2",
        "babel-preset-es2015": "^6.22.0",
        "webpack": "^2.2.1",
        "webpack-dev-server": "^2.4.1"
    }
}