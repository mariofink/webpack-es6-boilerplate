{
    "name": "webpack-es6-boilerplate",
    "version": "1.0.0",
    "description": "Write ES6/ES2015 code and compile to ES5 via Webpack",
    "scripts": {
        "build": "./node_modules/webpack/bin/webpack.js",
        "watch": "./node_modules/webpack/bin/webpack.js --watch",
        "devserver": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --content-base app"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mariofink/webpack-es6-boilerplate"
    },
    "keywords": [
        "webpack",
